mport numpy as np
from scipy.optimize import linprog


obj = [-1, -1.5]


lhs_ineq = [[-1, -1],
            [-1, 0]]


rhs_ineq = [-10,
            -3]


bnd = [(0, None),
       (0, None)]

opt = linprog(c=obj, A_ub=lhs_ineq, b_ub=rhs_ineq, bounds=bnd, method="highs")


if opt.success:
    print(f"Status: {opt.message}")
   
else:
    print(f"Status: {opt.message}")
